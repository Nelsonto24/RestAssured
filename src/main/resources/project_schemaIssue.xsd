<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="issue">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="xs:byte" name="id"/>
                <xs:element name="project">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute type="xs:byte" name="id"/>
                                <xs:attribute type="xs:string" name="name"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="tracker">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute type="xs:byte" name="id"/>
                                <xs:attribute type="xs:string" name="name"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="status">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute type="xs:byte" name="id"/>
                                <xs:attribute type="xs:string" name="name"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="priority">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute type="xs:byte" name="id"/>
                                <xs:attribute type="xs:string" name="name"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="author">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute type="xs:byte" name="id"/>
                                <xs:attribute type="xs:string" name="name"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element type="xs:string" name="subject"/>
                <xs:element type="xs:string" name="description"/>
                <xs:element type="xs:date" name="start_date"/>
                <xs:element type="xs:string" name="due_date"/>
                <xs:element type="xs:byte" name="done_ratio"/>
                <xs:element type="xs:string" name="is_private"/>
                <xs:element type="xs:string" name="estimated_hours"/>
                <xs:element type="xs:string" name="total_estimated_hours"/>
                <xs:element type="xs:float" name="spent_hours"/>
                <xs:element type="xs:float" name="total_spent_hours"/>
                <xs:element type="xs:dateTime" name="created_on"/>
                <xs:element type="xs:dateTime" name="updated_on"/>
                <xs:element type="xs:string" name="closed_on"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>